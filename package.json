{
  "name": "webpack-heroku-boilerplate",
  "version": "1.0.0",
  "description": "React apps.",
  "main": "app.js",
  "license": "MIT",
  "scripts": {
    "postinstall":"npm install -g webpack@2.6.1",
    "build": "NODE_ENV=production webpack --config ./webpack.prod.config.js --progress --colors",
    "start": "node app.js",
    "dev": "webpack-dev-server --hot --history-api-fallback --open",
    "rebuild": "npm run build && npm run start"
  },
  "dependencies": {
    "express": "^4.15.3",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.2",
    "node-sass": "^4.5.3",
    "react-hot-loader": "3.0.0-beta.6",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-split-chunks": "^0.2.1"
  }
}
